<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.JulyFNFUGHPack" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <item name="colorPrimary">#FFC500</item>
        <item name="colorPrimaryVariant">#FFC500</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">#FFC500</item>
        <item name="colorSecondaryVariant">#FFC500</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>
    <style name="MyTheme.JulyFNFUGHPack" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <item name="android:statusBarColor">#FFC500</item>
    </style>
    <!-- Set The Statusbar Color Style-->
    <style name="MyTheme.NoActionbarTheme" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <item name="android:statusBarColor">#FFD43F</item>
        <item name="android:windowLightStatusBar" tools:targetApi="m">true</item>
    </style>

    <style name="MyToolbar">
        <item name="android:background">#FFD43F</item>
        <item name="android:fontFamily">sans-serif</item>
        <item name="android:textColorHint">@color/white</item>
        <item name="titleTextColor">@color/white</item>
    </style>
</resources>